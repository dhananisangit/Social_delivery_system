 <script>

  var app = angular.module("schmuber_index_app", ['ui.bootstrap']);
  app.controller("schmuber_index_controller", function($scope,$http,$window,$location,$uibModal) {
    $scope.tripList = true;
    $scope.messageForm = false
    $scope.getHomeDetails = function(){
      $http({
        method:'GET',
        url:'/v1/getshipperrequests',
      }).then(function(res){
        $scope.deliveryDetails = res.data.shipperRequests
        // console.log($scope.deliveryDetails)
      })

    }

    $scope.sendMessage = function(){
      // $scope.tripList = false
      // $scope.messageForm = true;
          var modalInstance = $uibModal.open({
              ariaLabelledBy: 'modal-title',
              ariaDescribedBy: 'modal-body',
              templateUrl: 'QuoteContent',
              controller: 'QuoteModalCtrl',
              controllerAs: '$ctrl',
              resolve: {
                pn: function () {
                  return "hello";
                }
              }
          });


          modalInstance.result.then(function (message) {
            console.log(message)
          });
    }

});


angular.module('schmuber_index_app').controller('QuoteModalCtrl', function ($uibModalInstance,pn) {
    var $ctrl = this;
    $ctrl.pn=pn
    $ctrl.ok = function () {
        $uibModalInstance.close($ctrl.message);
    };

    $ctrl.cancel = function () {
        $uibModalInstance.dismiss('cancel');
    };
});
</script>
