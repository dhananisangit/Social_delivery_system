<div ng-init="fetchCustomerDetails()">
<div class="main">
  <div class="content">
  <section class="section  profile-header-wrapper  expand">
    <div class="content">
      <div class="profile-header">
        <a href="https://d1xqbpwl1wh09p.cloudfront.net/avatars/default/default.png" class="avatar  my-avatar  obj-label-over  profile__avatar  eb-profile-avatar" rel="lightbox">
          <img id="user_avatar" alt="Savan Thakker" src="https://d1xqbpwl1wh09p.cloudfront.net/avatars/default/default_130x130@2x.png" />
        </a><!-- end of profile avatar -->

        <h1 class="profile__title">
            {{customerDetails.firstName}} {{customerDetails.lastName}}
        </h1>
      </div><!-- profile-header -->
    </div>
  </section>
  <section class="section  profile__stats  expand">
    <div class="content cf">
      <div class="profile__stats-item">

          <span class="profile__stats-number">{{customerFeedback.length}}</span>
          <span class="profile__stats-reviews">Deliveries completed</span>

      </div>
      <div class="profile__stats-item">
        <span class="profile__stats-reviews">
          Joined {{customerDetails.dateJoined}}
        </span><!-- profile__stats-reviews -->
      </div><!-- profile__stats -->
    </div>
  </section>

  <section class="section  profile__content">
<div class="content  cf">


  <div class="user-verifications">
<h2 class="profile-content__title  user-verifications__title">
  Verified information
</h2>

<section class="obj-task-info  verifications-list">
<div class="data-list">
  <div class="data-list__item--clean">
    <div class="data-list__actions--left data-list__action--empty">
        <span class="obj-task-info__item-icon obj-task-info__item-icon--bank_account verified"></span>
    </div>
    <div class="data-list__info data-list__info--center">
          <h6 class="data-list__info__title">Verified user</h6>
          <span class="light-text-small">
             <span>{{customerDetails.firstName}} has provided identification and it’s verified by the Nimber team.</span>
          </span>
      </div>
    </div>
  </div>

<dl class="obj-task-info__item obj-task-info__item--first">
  <dt class="obj-task-info__item-title  cf">

    <span class="obj-task-info__item-title-text">Mobile number verified</span>
  </dt>
  <dd class="obj-task-info__item-content">
    <span class="obj-task-info__content-text  js-reload-page" rel="tooltip" title="">
        <span class="icon-font  details-icon  details-icon--yes" data-yes="✓" data-no="−">✓</span>
    </span>
  </dd>
</dl><!-- obj-task-info__item -->


<dl class="obj-task-info__item">
  <dt class="obj-task-info__item-title  cf">
    <span class="obj-task-info__item-title-text">Facebook connected</span>
  </dt>
  <dd class="obj-task-info__item-content">
    <span class="obj-task-info__content-text" rel="tooltip" title="">
        <span class="icon-font  details-icon  details-icon--yes" data-yes="✓" data-no="−">✓</span>
    </span>
  </dd>
</dl><!-- obj-task-info__item -->

<dl class="obj-task-info__item">
  <dt class="obj-task-info__item-title  cf">

    <span class="obj-task-info__item-title-text">Credit card connected</span>
  </dt>
  <dd class="obj-task-info__item-content">
    <span class="obj-task-info__content-text" rel="tooltip" title="">
        <span class="icon-font  details-icon  details-icon--yes" data-yes="✓" data-no="−">✓</span>
    </span>
  </dd>
</dl><!-- obj-task-info__item -->

<dl class="obj-task-info__item  obj-task-info__item--last">
  <dt class="obj-task-info__item-title  cf">

    <span class="obj-task-info__item-title-text">E-mail verified</span>
  </dt>
  <dd class="obj-task-info__item-content">
    <span class="obj-task-info__content-text">
      <span class="icon-font  details-icon  details-icon--yes" data-yes="✓" data-no="−">✓</span>
    </span>
  </dd>
</dl><!-- obj-task-info__item -->
</section>



</div><!-- user-verifications -->

    <div class="user-reviews">
<h2 class="profile-content__title">
    Reviews for {{customerDetails.firstName}}
</h2>
  <div class="user-reviews__breakdown">
    <div class="user-reviews__total">

<span class="user-rating user-rating--5 user-rating--big      " >
 {{customerFeedback.length}}
<span class="user-rating__icon">*</span>
</span>

        <b class="user-reviews__total-title">

          Reviews
        </b>

    </div><!-- user-reviews__breakdown-total -->
  </div><!-- user-reviews__breakdown -->

<ol class="unstyled" data-append-to="reviews-list">

<li class="user-review  cf">
<div class="user-review__content" ng-repeat="data in customerFeedback">
<span class="user-rating user-rating--5" >
  {{data.rating}}
  <span class="user-rating__icon">*</span>
</span>
    <article class="user-review__text">
        {{data.text}}
    </article>
    <div class="user-review__footer cf">
        <a href="" class="user-review__figure">
          <img id="user_avatar" alt="Manuel Rueda" src="https://d1xqbpwl1wh09p.cloudfront.net/avatars/default/default_40x40@2x.png" />
        </a><!-- end of profile avatar -->
      <div class="user-review__footer-content">
        <h3 class="user-review__title">
            <a href="">
             {{data.name}}<br/><br/>
            </a>
        </h3>
      </div><!-- user-review__footer-content -->
      <a href="/users/74441/reviews/18547" class="user-review__date">{{data.timestamp}}</a>
    </div><!-- user-review__foter -->
    <br/>
    <br/>
  </div><!-- user-review__content -->
</li><!-- user-review -->
    <!-- user-review -->
</ol>

    <div data-load="Loading...">
