 <script>

  var app = angular.module("schmuber_index_app", []);
  app.controller("schmuber_index_controller", function($scope,$http,$window,$location) {
    $scope.fetchCustomerDetails = function(){
      $http({
            method:'post',
            url:'/v1/getcustomerdetails',
        }).then(function(res){
        // console.log(res.data)
          $scope.customerFeedback = res.data.reviews[0].reviewsReceived
          // $scope.generic()
          $scope.customerProfileShow = true;
          $scope.customerDeliveries = Math.floor(Math.random()*5)
          $scope.customerDetails = res.data.details[0]
        });
    }

});

</script>
