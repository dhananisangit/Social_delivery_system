    <script>

  // The socketjs object
  var sock = null;

  // Opens a socket connection
  function openSocket(sessionID){
    sock = new SockJS('https://wspush.nimber.com/notifications');
    var sockPing = null;

    dings = null;

    if (typeof sessionID === 'undefined' || sessionID === null || sessionID.length === 0) {
      sessionID = '635782f0-afef-4425-a9d3-2714d3baadbf';
    }

    sock.onopen = function() {
      sock.send("authorize: " + sessionID);

      sockPing = window.setInterval(function() {
        sock.send('ping');
      }, 10000);
      $.publish('socket.open');
    };

    //fire question without socket
    if('' !== ''){
      var ask_question = JSON.parse('');
      if(ask_question !== undefined && ask_question !== null){
        e = {data: {event: "question:push", question: ask_question}, type: "manual_message"};
        show_notification_message(e);
      }
    }

    sock.onmessage = function(e) {
      if(e.data != "pong" && e.data.length > 0 && e.data !== null && e.data !== "[]") {
        if (typeof e.data === 'string') {
          var parsedData = JSON.parse(e.data);

          switch (parsedData.event) {
            case 'chatmessage:added':
              handleChatMessage(parsedData);
              break;
            case 'chatmessage:read':
              handleReadChatMessage(parsedData);
              break;
            case 'chatmessage:update_unread_count':
              handleUpdateUnreadCountChatMessage(parsedData);
              break;
            case 'job_action:created':
              handleJobActionCreated(parsedData);
              break;
            case 'job_action:updated':
              handleJobActionUpdated(parsedData);
              break;
            case 'job:released':
              handleJobReleased(parsedData);
              break;
            case 'job_action:canceled':
              handleJobActionCanceled(parsedData);
              break;
            case 'job_action:accepted':
              handleJobActionAccepted(parsedData);
              break;
            case 'job_action:declined':
              handleJobActionDeclined(parsedData);
              break;
            case 'transaction_state':
              handleTransactionState(parsedData);
              break;
            case 'job:bought':
              if (gon.global.taskId && gon.global.taskId == parsedData.id){
                window.location.reload();
              }
              break;
            case 'job:delivered':
              if (gon.global.taskId && gon.global.taskId == parsedData.id && parsedData.obj.driver.id == gon.global.current_user_id){
                window.location.reload();
              }
              break;
            case 'user:start_typing':
              handleUserStartTyping(parsedData);
              break;
            case 'user:online':
              handleUserState(parsedData);
              break;
            case 'user:offline':
              handleUserState(parsedData);
              break;
            case 'job:added':
            case 'job:buyable':
              // Do nothing
              break;
            default:
              show_notification_message(e);
          }
        }
      }
    };

    sock.onclose = function() {
      if (sockPing != null)
        window.clearInterval(sockPing);
    };

    // Close socket connection on unload
    $(window).unload(function(){
      sock.close();
    });
  }

