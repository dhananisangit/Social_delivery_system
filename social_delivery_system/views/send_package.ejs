      }).fail(function( jqxhr, settings, exception ) {console.log( exception );});
    });

  });

  $(window).load(function(){
  });

  //js parse the translations for profile.news to make them available in mustache templates
  t = JSON.parse('{"title":"Latest news","user_missing":"User missing","comment":" added a comment to the task ","bought":" said he would deliver ","creator":{"added":{"you_added":"You added the task"},"buyable":{"your_task":"The task","is_buyable":"is available"},"bought":{"your_package":"accepted your task"},"picked_up":{"your_package":"picked up the task"},"delivered_wait":{"your_package":"delivered the task"},"delivered":{"your_package":"The delivery of the task","confirmed":"was confirmed"},"canceled":{"your_package":"The task","deleted":"was deleted"},"released":{"your_task":"The bringer of your task","was_released":"was released"}},"sender":{"added":{"you_added":"You added the task"},"buyable":{"your_task":"The task","is_buyable":"is available"},"bought":{"your_package":"accepted the task"},"picked_up":{"your_package":"picked up the task"},"delivered_wait":{"your_package":"delivered the task"},"delivered":{"your_package":"The delivery of the task","confirmed":"was confirmed"},"canceled":{"your_package":"The task","deleted":"was deleted"}},"recipient":{"added":{"task":"added the task"},"buyable":{"your_task":"The task","is_buyable":"is available"},"bought":{"your_package":"accepted the task"},"picked_up":{"your_package":"picked up the task"},"delivered_wait":{"your_package":"delivered the task"},"delivered":{"your_package":"You confirmed that","confirmed":"was delivered"},"canceled":{"your_package":"The task","deleted":"was deleted"}},"driver":{"bought":{"pre":"You accepted the task","post":""},"picked_up":{"pre":"you picked up the task","post":""},"delivered_wait":{"pre":"You delivered the task","post":""},"delivered":{"pre":"The delivery of","post":"was confirmed"},"canceled":{"pre":"The task","post":"was canceled"}},"friend":{"buyable":"is now buyable and was created by","bought":"has bought","delivered":"has delivered","by":"by","now_buayable":"is now buyable"}}');

  //shows a notification/question
  function show_notification_message(e){
    //ignore ping messages
    if((e.data != "pong" && e.data.length > 0 && e.data !== null && e.data !== "[]") || e.type == "manual_message"){

      //parse message data and create template variables
      var data = e.data
      if(typeof(data) !== "object")
        var data = JSON.parse(e.data);
      var affiliation = "";
      if(data.obj !== undefined && data.obj.affiliation !== undefined)
        affiliation = data.obj.affiliation+"_";
      else
        affiliation = "";

      //find template id should be #notification_affiliation_object_event
      var $template = $("#notification_"+affiliation+data.event.replace(/:/g,"_"));
      var template_vars = new Object;
      template_vars.t = t;
      template_vars.entry = data;
      //logic if the message is a question
      var question = false;
      if(data.question !== undefined){
        question = true
        var $template = $("#notification_"+data.event.replace(/:/g,"_")+"_"+data.question.type);
        if(data.question.type === "answers"){
          for(var i=0; i<data.question.answers.length; i++){
            data.question.answers[i].index = i+1;
          }
        }
        template_vars.answer_url = Helpers.url_for("utils","answer_question",{question_id: data.question.id});

        if(typeof data.question.include_field !== 'undefined' || data.question.include_field !== null) {
          template_vars.entry.include_field_name = data.question.include_field;
          switch(data.question.include_field) {
            case 'job_id':
              template_vars.entry.include_field_value = null;
              break;
            default:
              template_vars.entry.include_field_value = null;
          }
        }
      }

      var image_size = "40x40";

      //create links to render in mustache templates
      if(data.obj !== undefined){
        if(data.obj.job !== undefined && data.obj.job !== null){
          template_vars.job_link = Helpers.url_for("tasks","show",data.obj.job.id);
          template_vars.job_image = Helpers.image_path(data.obj.job.picture_location,image_size);
        }
        if(data.obj.user !== undefined && data.obj.user !== null){
          template_vars.user_link = Helpers.url_for("users","show",data.obj.user.id);
          template_vars.user_image = Helpers.image_path(data.obj.user.avatar,image_size);
        }
        if(data.obj.user === undefined || data.obj.user === null){
          template_vars.user_link = Helpers.url_for("users","show",data.obj.creator.id);
          template_vars.user_image = Helpers.image_path(data.obj.creator.avatar,image_size);
        }
        if(data.obj.job === undefined || data.obj.job === null){
          template_vars.job_link = Helpers.url_for("tasks","show",data.obj.id);
          template_vars.job_image = Helpers.image_path(data.obj.picture_location,image_size);
        }
        if(data.obj.driver !== undefined && data.obj.driver !== null){
          template_vars.driver_link = Helpers.url_for("users","show",data.obj.driver.id);
          template_vars.driver_image = Helpers.image_path(data.obj.driver.avatar,image_size);
        }
        if(data.obj.sender !== undefined && data.obj.sender !== null){
          template_vars.sender_link = Helpers.url_for("users","show",data.obj.sender.id);
          template_vars.sender_image = Helpers.image_path(data.obj.sender.avatar,image_size);
        }
        if(data.obj.creator !== undefined && data.obj.creator !== null){
          template_vars.creator_link = Helpers.url_for("users","show",data.obj.creator.id);
          template_vars.creator_image = Helpers.image_path(data.obj.creator.avatar,image_size);
        }
      }
      //render template and fire notification
      if($template.length > 0){
        Notifications.show(Mustache.render($template.html(), template_vars),template_vars.answer_url, question);
      }
    }
  }

  function handleChatMessage(data) {
    // Publish a chatmesssage.added message, passing the relevant data to subscribers
    $.publish('chatmessage.added', {chat_id: data.chat_id, id: data.id, author_is_job_creator: data.author_is_job_creator});
  }

  function handleReadChatMessage(data) {
    /*
     * Fields in data:
     *   object: "ChatMessage"
     *   event: "chatmessage:read"
     *   chat_id: ID of the chat that was marked as read
     *   timestamp: Unix timestamp (UTC) of when the chat was marked as read
     */
    $.publish('chatmessage.read', {chat_id: data.chat_id, timestamp: data.timestamp});
  }

  /*
   * Fields in data:
   *   object: "User"
   *   event: "user:start_typing"
   *   chat_id: ID of the chat that typing activity was sent
   */
  function handleUserStartTyping(data){
    $.publish('chatmessage.typing', {chat_id: data.chat_id});
  }

  /*
   * Fields in data:
   *   object: "User"
   *   event: "user:online"
   *   user_id: id of the user whose state was requested
   */
  function handleUserState(data){
    if(data.event === 'user:online'){
      $.publish('user.state', {user_id: data.user_id, online: true});
    }else {
      $.publish('user.state', {user_id: data.user_id, online: false});
    }
  }

  function handleUpdateUnreadCountChatMessage(data) {
    /*
     * Fields in data:
     *   object: "ChatMessage"
     *   event: "chatmessage:update_unread_count"
     *   chat_id: ID of the chat that was marked as read
     *   timestamp: Unix timestamp (UTC) of when the chat was marked as read
     */

    // Update the counter in all open windows
    // $.get('/chats/get_unread', function(unreadChats) {
    //   TaskChat.markedAsRead(data.chat_id, unreadChats);
    // });
  }

  function handleJobActionCreated(data) {
    /*
     * Fields in data:
     *   object: "JobAction"
     *   event: "job_action:created"
     *   id: ID of the job action (offer)
     *   job_id: ID of the job
     *   timestamp: Unix timestamp (UTC) of when the event was sent
     *   user_id: ID of job_action creator
     */

    if (gon.global.taskId && gon.global.taskId == data.job_id && (gon.global.chatUser == null || data.user_id == gon.global.chatUser)) {
      window.location.reload();
    }
  }

  function handleJobActionUpdated(data) {
    /*
     * Fields in data:
     *   object: "JobAction"
     *   event: "job_action:updated"
     *   id: ID of the job action (offer)
     *   job_id: ID of the job
     *   timestamp: Unix timestamp (UTC) of when the event was sent
     */

    if (gon.global.taskId && gon.global.taskId == data.job_id && $('#offer_' + data.id).length > 0) {
      window.location.reload();
    }
  }

  function handleJobReleased(data) {
    /*
     * Fields in data:
     *   object: "Job"
     *   event: "job:released"
     *   id: ID of the job
     *   timestamp: Unix timestamp (UTC) of when the event was sent
     */

    if (gon.global.taskId && gon.global.taskId == data.id) {
      window.location.reload();
    }
  }

  function handleJobActionCanceled(data) {
    /*
     * Fields in data:
     *   object: "Job"
     *   event: "job:released"
     *   id: ID of the job
     *   job_id: ID of the job
     *   timestamp: Unix timestamp (UTC) of when the event was sent
     */

    if (gon.global.taskId && gon.global.taskId == data.job_id && $('#offer_' + data.id).length > 0) {
      window.location.reload();
    }
  }

  function handleJobActionAccepted(data) {
    /*
     * Fields in data:
     *   object: "JobAction"
     *   event: "job_action:accepted"
     *   id: ID of the job action (offer)
     *   job_id: ID of the job
     *   timestamp: Unix timestamp (UTC) of when the event was sent
     */

    if (gon.global.taskId && gon.global.taskId == data.job_id) {
      window.location.reload();
    }
  }

  function handleJobActionDeclined(data) {
    /*
     * Fields in data:
     *   object: "JobAction"
     *   event: "job_action:accepted"
     *   id: ID of the job action (offer)
     *   job_id: ID of the job
     *   timestamp: Unix timestamp (UTC) of when the event was sent
     */

    if (gon.global.taskId && gon.global.taskId == data.job_id && $('#offer_' + data.id).length > 0) {
      window.location.reload();
    }
  }
  function handleTransactionState(data){
    $.publish('transaction_state:updated', {task_id: data.job_id, state: data.state });
  }

</script>

